{
    "compilerOptions": {
        // "allowSyntheticDefaultImports": true,
        "target": "es5", //设置ts编译的目标版本：ES3（默认）、ES5、ES6/ES2015、ES7/ES2016、ES2017、ES2018、ES2019、ES2020、ESNext
        "module": "umd", //指定要使用的模块化规范:CommonJS、UMD、AMD、System、ES2020、ESNext、None  es6,es2015
        // "lib": ["es5", "es6","es2016"], // 指定代码运行时所包含的库（宿主环境,运行在浏览器的js程序不需要设置此项）：ES5、ES6/ES2015、ES7/ES2016、ES2017、ES2018、ES2019、ES2020、ESNext、DOM、WebWorker、ScriptHost 
        "lib": ["ES5","DOM"],
        "outDir": "./lib", //编译后文件输出目录
        // "outFile": "dist/aa.js", // None、System或AMD
        "declaration": true, // 是否生成d.ts文件
        // "moduleResolution": "node",
        // "esModuleInterop": true,
        // "skipLibCheck": true,
        // "allowJs": true //allowJs: 是否对js文件进行编译，默认为false
        // "checkJs":true,// 是否对js文件进行检查
        // "removeComments": true, // 是否移除注释, 默认false
        // "module": "commonjs",
        // "sourceMap": true
    },
    "files": ["src/index.ts"] //指定被编译文件的列表
}
